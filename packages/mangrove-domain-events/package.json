{
  "name": "@proximaone/stream-schema-mangrove-domain-model",
  "version": "0.1.0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "yarn build",
    "gen": "prisma generate",
    "prepare": "yarn gen",
    "prebuild": "yarn clean && yarn format && yarn lint && echo Using TypeScript && tsc --version",
    "build": "tsc --project tsconfig.build.json --pretty",
    "test": "jest --passWithNoTests",
    "coverage": "jest --coverage",
    "watch": "yarn build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "@prisma/client": "3.11.0",
    "@proxima-one/stream-schema-base": "^0.1.2",
    "@proximaone/stream-client-js": "^0.4.0",
    "@types/graphql-fields": "^1.3.4",
    "apollo-server": "^3.6.3",
    "bignumber.js": "^9.0.2",
    "class-validator": "^0.13.2",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.16.0",
    "i": "^0.3.7",
    "lodash": "^4.17.21",
    "node-fetch-cache": "^3.0.5",
    "prisma": "^3.11.1",
    "prisma-json-schema-generator": "^2.0.7",
    "prisma-typegraphql-types-generator": "^1.5.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4",
    "tslib": "^2.3.1",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.19.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "preset": "<rootDir>/"
  }
}
