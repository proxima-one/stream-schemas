{
  "name": "@proximaone/stream-schema-eth-block",
  "version": "0.1.8",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\"",
    "prepublishOnly": "yarn build",
    "generate": "buf generate",
    "prepare": "yarn build",
    "prebuild": "yarn clean && yarn format && echo Using TypeScript && tsc --version",
    "build": "yarn clean && yarn compile && yarn copy-content-files",
    "test": "jest --passWithNoTests",
    "copy-content-files": "copyfiles -u 1 src/**/*.proto dist/",
    "compile": "tsc --build",
    "coverage": "jest --coverage",
    "watch": "yarn build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "@proximaone/stream-schema-base": "^0.1.11",
    "@grpc/grpc-js": "^1.6.7"
  },
  "devDependencies": {
    "ts-proto": "^1.110.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.18.4",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "<rootDir>/"
  }
}
