{
  "name": "@proximaone/stream-schema-eth-block-events",
  "version": "0.1.4",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "yarn build",
    "prepare": "yarn build",
    "prebuild": "yarn clean && yarn format && yarn lint && echo Using TypeScript && tsc --version",
    "build": "yarn clean && yarn compile && yarn copy-content-files",
    "test": "jest --passWithNoTests",
    "copy-content-files": "copyfiles -u 1 src/**/*.proto dist/",
    "compile": "tsc --build",
    "coverage": "jest --coverage",
    "watch": "yarn build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@proximaone/stream-client-js": "^0.4.0",
    "@proximaone/stream-schema-base": "^0.1.11",
    "bignumber.js": "^9.0.2",
    "i": "^0.3.7",
    "lodash": "^4.17.21",
    "node-fetch-cache": "^3.0.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4",
    "ts-proto": "^1.110.4",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "<rootDir>/"
  }
}
